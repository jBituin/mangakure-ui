{
  "name": "mangakure-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "test:unit": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "vue-jest": {
        "transform": {
          "your-custom-block": "./custom-block-processor.js"
        }
      }
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|specs))\\.(ts)$"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "nuxt": "^2.14.6",
    "vue-infinite-scroll": "^2.0.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/vuetify": "^1.11.2",
    "@types/jest": "^26.0.16",
    "@vue/test-utils": "^1.1.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.6.12"
  }
}
